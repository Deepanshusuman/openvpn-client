QScrollArea{	border: none;}QScrollBar:vertical {    border: none !important;		border-left: 1px solid  #cccccc !important;    background: transparent;    width: 15px;    margin: 25px 0 25px 0;}QScrollBar::handle:vertical {    background:transparent;	border: 1px solid #cccccc;	border-left: none;    min-height: 20px;}QScrollBar::add-line:vertical {        height: 23px;	border: 1px solid #cccccc;    subcontrol-position: bottom;    subcontrol-origin: margin;}QScrollBar::sub-line:vertical {	    height: 23px;	border: 1px solid #cccccc;    subcontrol-position: top;    subcontrol-origin: margin;}QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {    border: 1px solid #cccccc;    width: 3px;    height: 3px;    background: white;}QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {    background: none;}QScrollBar:horizontal {    border: none;		border-top: 1px solid  #cccccc;    background: transparent;    height: 15px;    margin: 0 25px 0 25px;}QScrollBar::handle:horizontal {    background:transparent;	border: 1px solid #cccccc;	border-top: none;    min-width: 20px;}QScrollBar::add-line:horizontal {        width: 23px;	border: 1px solid #cccccc;    subcontrol-position: right;    subcontrol-origin: margin;}QScrollBar::sub-line:horizontal {	    width: 23px;	border: 1px solid #cccccc;    subcontrol-position: left;    subcontrol-origin: margin;}QScrollBar::left-arrow:horizontal, QScrollBar::right-arrow:horizontal {    border: 1px solid #cccccc;    width: 3px;    height: 3px;    background: white;}QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {    background: none;}